    </link>
    <gazebo reference="${name}_REFRIGERATOR-BODYSET2_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>
    <link name="${name}_REFRIGERATOR-BODYSET7_link">
