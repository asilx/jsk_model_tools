      <child link="${name}_REFRIGERATOR-BODYSET5_link"/>
      <origin xyz="0.035 0 0.805" rpy="0 -0 0 "/>
      <axis xyz="1 0 0"/>
      <limit lower="0" upper="0.5" effort="100" velocity="0.01"/>
