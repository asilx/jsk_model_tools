<?xml version='1.0' encoding='utf-8'?>
<robot xmlns:xi="http://www.w3.org/2001/XInclude" name="room73a3-officeprinter">
  <macro name="room73a3-officeprinter" params="name parent *origin">
    <gazebo>
      <static>false</static>
    </gazebo>
    <link name="${name}_PRINTER_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room73a3-officeprinter/meshes/PRINTER_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room73a3-officeprinter/meshes/PRINTER_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.001"/>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <inertia ixx="1e-03" ixy="0" ixz="0" iyy="1e-03" iyz="0" izz="1e-03"/>
      </inertial>
    </link>
    <gazebo reference="${name}_PRINTER_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>
    <link name="${name}_SCANNER_link">
      <visual>
        <origin xyz="0 0 0.984" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room73a3-officeprinter/meshes/SCANNER_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0.984" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room73a3-officeprinter/meshes/SCANNER_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.001"/>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <inertia ixx="1e-03" ixy="0" ixz="0" iyy="1e-03" iyz="0" izz="1e-03"/>
      </inertial>
    </link>
    <gazebo reference="${name}_SCANNER_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>
    <joint name="${name}_root_parent_joint" type="fixed">
      <parent link="${parent}"/>
      <child link="${name}_PRINTER_link"/>
      <insert_block name="origin"/>
    </joint>
    <joint name="${name}_JOINT0" type="revolute">
      <parent link="${name}_PRINTER_link"/>
      <child link="${name}_SCANNER_link"/>
      <origin xyz="0 0 0.984" rpy="0 -0 0 "/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="3.14159" effort="100" velocity="0.5"/>
      <dynamics damping="0.2" friction="0"/>
    </joint>
    <transmission type="pr2_mechanism_model/SimpleTransmission" name="${name}_JOINT0_trans">
      <actuator name="${name}_JOINT0_motor"/>
      <joint name="${name}_JOINT0"/>
      <mechanicalReduction>1</mechanicalReduction>
    </transmission>
    <gazebo reference="${name}_JOINT0">
      <cfmDamping>0.4</cfmDamping>
    </gazebo>
  </macro>
</robot>
