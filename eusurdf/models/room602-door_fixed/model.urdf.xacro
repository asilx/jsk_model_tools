<?xml version='1.0' encoding='utf-8'?>
<robot xmlns:xi="http://www.w3.org/2001/XInclude" name="room602-door">
  <macro name="room602-door" params="name parent *origin">
    <gazebo>
      <static>false</static>
    </gazebo>
    <link name="${name}_ROOM602-DOOR-BODYSET2_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room602-door/meshes/ROOM602-DOOR-BODYSET2_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room602-door/meshes/ROOM602-DOOR-BODYSET2_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.001"/>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <inertia ixx="1e-03" ixy="0" ixz="0" iyy="1e-03" iyz="0" izz="1e-03"/>
      </inertial>
    </link>
    <gazebo reference="${name}_ROOM602-DOOR-BODYSET2_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>
    <link name="${name}_ROOM602-DOOR-BODYSET3_link">
      <visual>
        <origin xyz="-0.017 -0.017 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room602-door/meshes/ROOM602-DOOR-BODYSET3_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="-0.017 -0.017 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="model://room602-door/meshes/ROOM602-DOOR-BODYSET3_link_mesh.dae" scale="1 1 1"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.001"/>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <inertia ixx="1e-03" ixy="0" ixz="0" iyy="1e-03" iyz="0" izz="1e-03"/>
      </inertial>
    </link>
    <gazebo reference="${name}_ROOM602-DOOR-BODYSET3_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>
    <joint name="${name}_root_parent_joint" type="fixed">
      <parent link="${parent}"/>
      <child link="${name}_ROOM602-DOOR-BODYSET2_link"/>
      <insert_block name="origin"/>
    </joint>
    <joint name="${name}_DOOR" type="revolute">
      <parent link="${name}_ROOM602-DOOR-BODYSET2_link"/>
      <child link="${name}_ROOM602-DOOR-BODYSET3_link"/>
      <origin xyz="-0.017 -0.017 0" rpy="0 -0 0 "/>
      <axis xyz="0 0 1"/>
      <limit lower="0" upper="1.5708" effort="100" velocity="0.5"/>
      <dynamics damping="0.2" friction="0"/>
    </joint>
    <transmission type="pr2_mechanism_model/SimpleTransmission" name="${name}_DOOR_trans">
      <actuator name="${name}_DOOR_motor"/>
      <joint name="${name}_DOOR"/>
      <mechanicalReduction>1</mechanicalReduction>
    </transmission>
    <gazebo reference="${name}_DOOR">
      <cfmDamping>0.4</cfmDamping>
    </gazebo>
  </macro>
</robot>
